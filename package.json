{
  "name": "commento-react-material-ui",
  "version": "1.2.4-beta",
  "description": "A react based frontend for Commento baked with material-ui",
  "author": "rbrishabh",
  "license": "MIT",
  "repository": "rbrishabh/commento-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs && npm run copy:css",
    "copy:css": "cp ./src/commento-style.css ./dist",
    "watch:css": "node watchcss.js",
    "start": "concurrently \"microbundle-crl watch --no-compress --format modern,cjs\" \"npm run watch:css\"",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.5.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@types/react-scroll": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5",
    "concurrently": "^5.2.0",
    "chokidar": "^3.4.0",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.5.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/classnames": "^2.2.10",
    "@types/lodash.clonedeep": "^4.5.6",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.27.0",
    "react-scroll": "^1.7.16",
    "turndown": "^6.0.0"
  }
}
